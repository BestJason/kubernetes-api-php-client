{
    "name": "devstub/kubernetesAPI-PHP-client",
    "description": "PHP client for the kubernetes api",
    "minimum-stability": "stable",
    "license": "Apache-2.0",
    "keywords": ["kubernetes", "api", "client", "restful", "http"],
    "authors": [
        {
            "name": "Faruk Brbovic",
            "email": "fbrbovic@devstub.com",
            "homepage": "http://www.devstub.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/devstub/jsonmapper.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "5.0.3",
        "netresearch/jsonmapper": "dev-issue_8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "DevStub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevStub\\Tests\\": "tests/"
        }
    }
}
